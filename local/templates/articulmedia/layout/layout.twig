{% block sidenav %}
<md-sidenav class="sidenav md-sidenav-right"
  layout="column"
  md-component-id="sidenav-right"
  md-whiteframe="4"
  md-is-open="sidenavIsOpen"
  >
  <md-button class="md-raised sidenav__close md-mini" ng-click="sidenavToggle()" aria-label="menu" md-no-ink="true|false" md-ripple-size="auto" >
    <span class="sidenav__nav-item__icon svg-icon">
      <svg>
        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#spr-close" />
      </svg>
    </span>
  </md-button>
</md-sidenav>
{% endblock %}

{% block main %}
<ui-view></ui-view>
